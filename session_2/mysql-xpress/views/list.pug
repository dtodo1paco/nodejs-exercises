extends layout
block body
  if message
    if message.error
      div.alert.alert-danger #{message.error}
    else
      if message.success
        div.alert.alert-success #{message.success}
  div(class="container")
    table
      tbody
        each order in orders
          tr
            td #{order.datetime}
            td #{order.name}
            td #{order.quantity}

      
